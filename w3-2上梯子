#斐波拉契数列
#include "stdafx.h"
#include<iostream>
#include<string.h>
#include<cstdio>
#include<algorithm>
using namespace std;
int N;
int stairs(int n)
{
	if (n == 1)
		return 1;
	else if (n == 2)
		return 2;
	else
		return stairs(n - 1) + stairs(n - 2);
}

int main()
{
	while (cin >> N)
	{
		cout << stairs(N) << endl;
	}
	return 0;
}
